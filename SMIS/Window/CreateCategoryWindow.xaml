<Window x:Class="SMIS.CreateCategoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SMIS"
        mc:Ignorable="d"
        Title="Create Category" Height="150" Width="300" Background="#FF272727" ResizeMode="NoResize">
    <Grid>
        <Label Content="분류이름:" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" Foreground="White"/>
        <TextBox x:Name="txt_categoryName" Height="23" Margin="76,33,10,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Background="#FF3F3F46" Foreground="White"/>
        <Button x:Name="btn_ok" Content="확인" Margin="110,79,110,0" VerticalAlignment="Top" Click="btn_ok_Click"/>

    </Grid>
</Window>
